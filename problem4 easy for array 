#include <iostream>
using namespace std;
int
main ()
{
  int arr[1000], n;
  cin >> n;
  for (int i = 0; i < n; i++)
	{
	  cin >> arr[i];
	}

  for (int i = 0; i < n; i++)
	{
	  if (arr[i] == arr[n - i - 1])
		cout << "YES";
	  if (arr[i] != arr[n - i - 1])
		cout << "NO";
	  break;
	}

  return 0;
}
